{
    "name": "tablegenerator",
    "displayName": "Markdown TableGenerator",
    "description": "Generate formatted tables for markdown",
    "version": "1.0.0",
    "publisher": "JayFi Development",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.generateTable",
        "onCommand:extension.generateTableWithAlignment"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateTable",
                "title": "Markdown: New Table"
            },
            {
                "command": "extension.generateTableWithAlignment",
                "title": "Markdown: New Table With Alignment"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
